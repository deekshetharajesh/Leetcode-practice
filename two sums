class Solution {
    public int[] twoSum(int[] nums, int target) {
         int n=nums.length;
        int b;
        for(int i=0;i<n-1;i++)
        {
            for(int j=i+1;j<n;j++)
            {
                b=nums[i]+nums[j];
                if(b==target)
                {
                    return new int[]{i,j};
                }
            }
              }return new int[]{};
    }
    }
